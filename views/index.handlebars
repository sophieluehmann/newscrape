<ul>
    <div class="container">
      {{#each Article}}
      <div class="card border-dark mb-4 w-75 shadow p-3 mb-5 bg-white rounded">
        <div class="card-body">
           <h5 class="mr-auto">
            <a class="article-link" href={{this.link}}>{{this.title}}</a>
            </h5>
            <span>
            <button class="btn btn-primary" id="saved">
              <a name="Saved-Article" style="color: #fff" title={{this.title}} link={{this.link}}>Save</a> 
            </button>
            </span> 
            
            
        </div>
      </div> 
      {{/each}}  
    </div>
    
</ul>
    